{
  "manifest_version": 2,
  "name": "GraphQL request viewer",
  "version": "2.0",
  "description": "A better request viewer for GraphQL requests in chrome and firefox dev tools",
  "devtools_page": "devtools.html",
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./static/js/content.js"],
      "all_frames": false,
      "run_at": "document_start"
    }
  ]
}
